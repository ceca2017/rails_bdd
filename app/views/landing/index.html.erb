<ul>
  <% @articles.each do |article| %>
    <li class = "article">
      <%= article.title %>
      <%= #link_to article.title, article_path(article) %><br />
      <%= article.content %><br />
      Written by <%= article.author %> at <%= article.created_at.strftime("%Y-%m-%d") %>
    </li>
  <% end %>
</ul>
